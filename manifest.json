{
    "name": "Emu",
    "manifest_version": 2,
    "version": "0.0.3",
    "description": "Webpage cutting and collecting",
    "permissions": [
        "<all_urls>"
    ],
    "icons": {
        "16": "icons/16.png",
        "24": "icons/24.png",
        "32": "icons/32.png",
        "48": "icons/48.png"
    },
    "browser_action": {
        "default_title": "Emu",
        "default_icon": "icons/24.png"
    },
    "background": {
        "scripts": [
            "index.js"
        ]
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "css": ["style.css"],
        "js": ["lib/jquery-2.0.3.min.js", "contentScript.js"]
    }]
}
